<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.kosta.mapper.messengerMapper">

	<insert id="postMessenge">
		INSERT INTO msg
		VALUES(mgs_seq.nextval, #{msg_content }, #{msg_sender }, #{msg_projectId }, SYSDATE)
	</insert>
	
</mapper>
